.btn.btn-default {
  width: 18.125rem;
  padding: 1.25rem 0;
  font-size: 1.25rem;
  font-weight: 500;
}

.btn.btn-default .btn-line-inner {
  padding: 1.0625rem 0;
  box-sizing: content-box;
}

.btn.btn-line {
  padding: 0.1875rem;
  background-image: linear-gradient(to right, #490097, #9e0a96);
  color: #490097;
}

.btn.btn-line .btn-line-inner {
  display: inline-block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background-color: #ffffff;
}

.btn.btn-round {
  border-radius: 1.875rem;
  overflow: hidden;
}

.btn.btn-round .btn-line-inner {
  border-radius: 1.875rem;
}

.btn.btn-primary-lighten {
  color: #fff;
  background: #9e0a96;
}

.btn.btn-secondary {
  color: #fff;
  background: #ed1651;
}

.btn .icon-plus {
  margin-left: 0.625rem;
}

.btn.w100 {
  width: 100%;
  padding: 1.0625rem 0;
  box-sizing: border-box;
}

.btn-wrap {
  text-align: center;
}

.btn-wrap .btn+.btn {
  margin-left: 0.625rem;
}

.btn-wrap.ea2 .btn {
  width: calc(50% - 0.3125rem);
}

.btn-wrap.ea3 .btn {
  width: calc(33% - 0.3125rem);
}

@media screen and (max-width: 992px) {
  .btn.btn-default {
    width: 100%;
    padding: 1.125rem 0;
    font-size: 1.75rem;
    font-weight: 500;
  }
  .btn.btn-default .btn-line-inner {
    padding: 0.875rem 0;
    box-sizing: content-box;
  }
  .btn.btn-round {
    border-radius: 3.125rem;
    overflow: hidden;
  }
  .btn.btn-line {
    padding: 0.25rem;
  }
  .btn.btn-line .btn-line-inner {
    border-radius: 3.125rem;
  }
  .btn-wrap {
    text-align: center;
  }
  .btn-wrap .btn+.btn {
    margin: 0;
  }
  .btn-wrap.ea2 .btn,
  .btn-wrap.ea3 .btn {
    margin: 0 0 1.25rem -0.25rem;
    width: 100%;
    padding-left: 0.25rem;
    padding-right: 0.25rem;
  }
}

input,
textarea,
button {
  font-family: 'Noto Sans KR', sans-serif;
  letter-spacing: -0.025em;
  border: 0;
}

input:disabled {
  background: #f9f9f9;
}

input::placeholder,
textarea::placeholder {
  color: #999999;
  font-family: 'Noto Sans KR', sans-serif;
}

input::-webkit-input-placeholder {
  color: #999999;
  font-family: 'Noto Sans KR', sans-serif;
}

input:-ms-input-placeholder {
  color: #999999;
  font-family: 'Noto Sans KR', sans-serif;
}

textarea::-webkit-input-placeholder {
  color: #999999;
  font-family: 'Noto Sans KR', sans-serif;
}

textarea:-ms-input-placeholder {
  color: #999999;
  font-family: 'Noto Sans KR', sans-serif;
}

.input-text {
  border: 1px solid #d5d5d5;
}

.input-text input[type="text"],
.input-text input[type="password"] {
  display: block;
  height: 100%;
  width: 100%;
  padding: 0.9375rem 1.25rem;
  color: #333;
  box-sizing: border-box;
}

.input-text.has-btn {
  position: relative;
  padding-right: 9.375rem;
}

.input-text.has-btn button {
  position: absolute;
  height: 100%;
  width: 9.375rem;
  right: 0;
  top: 0;
  padding: 0.9375rem;
  color: #ffffff;
  background-color: #999999;
}

.input-textarea {
  border: 1px solid #d5d5d5;
}

.input-textarea textarea {
  display: block;
  height: 100%;
  width: 100%;
  padding: 0.9375rem 1.25rem;
  color: #333;
  border: 0;
  box-sizing: border-box;
  resize: none;
  font-family: 'Noto Sans KR', sans-serif;
  overflow-y: auto;
}

.input-file {
  border: 1px solid #d5d5d5;
}

.input-file .dumy-input {
  position: relative;
  height: 100%;
  width: 100%;
  padding: 0.9375rem 1.25rem;
  box-sizing: border-box;
}

.input-file .dumy-input button {
  position: absolute;
  height: 100%;
  width: 9.375rem;
  right: 0;
  top: 0;
  padding: 0.9375rem;
  color: #ffffff;
  background-color: #999999;
}

.input-file input[type="file"] {
  position: absolute;
  font-size: 0;
  height: 0;
  width: 0;
}

.chk-wrap input[type="checkbox"] {
  display: none;
}

.chk-wrap label,
.chk-wrap .icon-chk {
  display: inline-block;
  vertical-align: middle;
  margin: 0 0.3125rem;
  cursor: pointer;
}

.radio-wrap input[type="radio"] {
  display: none;
}

.radio-wrap label,
.radio-wrap .icon-chk {
  display: inline-block;
  vertical-align: middle;
  margin: 0 0.3125rem;
  cursor: pointer;
}

.radio-wrap label {
  display: inline;
}

.select-wrap {
  position: relative;
  border: 1px solid #d5d5d5;
}

.select-wrap select {
  width: 100%;
  line-height: 3.0625rem;
  text-indent: 1.25rem;
  font-size: 1.125rem;
  background-color: #fff;
}

.select-wrap select option {
  text-indent: 1.25rem;
}

.select-wrap select::-ms-expand {
  display: none;
}

.select-wrap:after {
  content: '';
  overflow: hidden;
  display: inline-block;
  width: 0.625rem;
  height: 0.625rem;
  border-top: 0.125rem solid #333;
  border-left: 0.125rem solid #333;
  transform: rotate(225deg);
  position: absolute;
  top: 0.9375rem;
  right: 0.9375rem;
  border-color: #d5d5d5;
}

@media screen and (max-width: 992px) {
  .chk-wrap {
    margin: 1.875rem 0;
  }
  .chk-wrap label {
    font-size: 1.5rem;
  }
  .select-wrap select {
    width: 100%;
    text-indent: 1.25rem;
    line-height: 4.375rem;
    font-size: 1.5rem;
  }
  .select-wrap:after {
    top: 1.5625rem;
    right: 1.25rem;
    border-width: 0.1875rem;
  }
}

.modal-wrap {
  visibility: hidden;
  opacity: 0;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2000;
  background: rgba(0, 0, 0, 0.5);
  width: 100vw;
  height: 100vh;
  transition: all 0.3s;
}

.modal-wrap.is-visible {
  visibility: visible;
  opacity: 1;
}

.modal-wrap .modal {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  min-width: 31.25rem;
  min-height: 20.625rem;
  max-width: 90vw;
  max-height: 90vh;
  padding: 3.125rem;
  box-sizing: border-box;
  background: #fff;
  border-radius: 0.25rem;
  overflow-y: auto;
}

.modal-wrap .modal .title {
  font-size: 1.25rem;
  font-weight: 500;
  color: #490097;
  border-bottom: 0.0625rem solid #d5d5d5;
  margin: -1.25rem -3.125rem 0;
  padding: 0rem 1.875rem 1.5625rem;
}

.modal-wrap .modal .con .txt {
  font-size: 1.125rem;
  margin: 3.125rem 0 3.75rem;
  line-height: 1.6;
}

.modal-wrap .modal .con .str {
  font-weight: 500;
}

.modal-wrap .modal .con .str.l-txt {
  display: block;
  padding: 0.9375rem 0;
  font-size: 1.5rem;
}

.modal-wrap .modal .con .btn {
  width: 100%;
}

.modal-wrap .modal .con .btn-wrap.ea2 .btn {
  width: calc(50% - 0.9375rem);
}

.modal-wrap .modal .con .btn-wrap.ea3 .btn {
  width: calc(33% - 0.9375rem);
