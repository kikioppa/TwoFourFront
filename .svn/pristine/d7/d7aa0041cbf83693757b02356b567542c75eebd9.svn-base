/* http://meyerweb.com/eric/tools/css/reset/ 
   v2.0 | 20110126
   License: none (public domain)
*/

@import url("https://fonts.googleapis.com/css?family=Noto+Sans+KR:400,500,700&display=swap");
@import url("https://fonts.googleapis.com/css?family=Open+Sans:400,600,700&display=swap");
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
}


/* HTML5 display-role reset for older browsers */

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}

body {
  line-height: 1;
}

ol,
ul {
  list-style: none;
}

blockquote,
q {
  quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
  content: '';
  content: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}


/* add */

a {
  text-decoration: none;
  color: inherit;
}

button {
  background: transparent;
  padding: 0;
  margin: 0;
  border: 0;
  outline: none;
}

img {
  border: 0;
  padding: 0;
  margin: 0;
  font-size: 0;
}

input[type="text"] {
  padding: 0;
  border: 0;
}

html {
  font-size: 16px;
  color: #333;
}

body {
  font-family: 'Noto Sans KR', sans-serif;
  font-weight: 400;
  letter-spacing: -0.025em;
  font-size: 1rem;
}

.f-wrap:after {
  display: block;
  content: '';
  clear: both;
}

.f-wrap .left {
  display: block;
  float: left;
}

.f-wrap .right {
  display: block;
  float: right;
}

img {
  max-width: 100%;
}

.hidden {
  font-size: 0;
  text-indent: -9999px;
  overflow: hidden;
}

button {
  cursor: pointer;
}

.btn {
  display: inline-block;
  text-align: center;
}

.btn.btn-gradient {
  display: block;
  width: 15.625rem;
  height: 3.375rem;
  line-height: 3.375rem;
  position: relative;
  background: #fff;
  color: #490097;
  border: 0rem solid transparent;
}

.btn.btn-gradient:before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: -1;
  margin: -0.1875rem;
  border-radius: inherit;
  background-image: linear-gradient(to right, #490097, #9e0a96);
  -ms-background-image: linear-gradient(to right, #490097, #9e0a96);
  -o-background-image: linear-gradient(to right, #490097, #9e0a96);
  -webkit-background-image: linear-gradient(to right, #490097, #9e0a96);
  background-color: #490097;
}

.btn.btn-round {
  border-radius: 1.875rem;
}

.shadow {
  box-shadow: 0.9375rem 0.9375rem 1.25rem rgba(0, 0, 0, 0.15);
}

.page-down {
  position: absolute;
  left: 50%;
  transform: translate(-50%, 0%);
  bottom: 5.625rem;
  width: 3.75rem;
  height: 3.75rem;
  background: #a47fcb;
  border-radius: 100%;
  text-align: center;
  z-index: 10;
  box-shadow: 0.5rem 0.5rem 0.625rem rgba(0, 0, 0, 0.15);
}

.page-down .arrow-d {
  overflow: hidden;
  display: inline-block;
  width: 0.625rem;
  height: 0.625rem;
  border-top: 0.125rem solid #333;
  border-left: 0.125rem solid #333;
  transform: rotate(225deg);
  border-color: #fff;
  width: 0.75rem;
  height: 0.75rem;
  border-width: 0.1875rem;
  border-radius: 0.1875rem;
}

.btn-contact {
  position: fixed;
  z-index: 10;
  left: 3.125rem;
  bottom: 2.5rem;
  width: 5rem;
  height: 5rem;
  border-radius: 100%;
  background-image: -ms-linear-gradient(-30deg, #fdb94d, #ed1651);
  background-image: -o-linear-gradient(-30deg, #fdb94d, #ed1651);
  background-image: linear-gradient(-30deg, #fdb94d, #ed1651);
  background-color: #ed1651;
}

.btn-contact.absol {
  position: absolute;
  bottom: 1.25rem;
}

.btn-contact .icon-mail {
  margin-top: 0.25rem;
}

.btn-plus {
  display: inline-block;
  width: 2.5rem;
  height: 2.5rem;
  line-height: 2.375rem;
  border-radius: 100%;
  text-align: center;
  color: #fff;
  font-size: 1.75rem;
  background-image: linear-gradient(to right, #490097, #9e0a96);
  -ms-background-image: linear-gradient(to right, #490097, #9e0a96);
  -o-background-image: linear-gradient(to right, #490097, #9e0a96);
  -webkit-background-image: linear-gradient(to right, #490097, #9e0a96);
  background-color: #490097;
}

.disc-list {
  display: inline-block;
}

.disc-list li {
  position: relative;
  text-align: left;
  padding: 0 0 0 0.625rem;
  margin: 1.25rem 0;
  font-size: 1rem;
  color: #787878;
}

.disc-list li:before {
  content: '';
  width: 0.1875rem;
  height: 0.1875rem;
  border-radius: 100%;
  background: #490097;
  position: absolute;
  top: 0.625rem;
  left: 0;
}

.title-bar {
  display: block;
  text-align: left;
  font-size: 1.375rem;
  font-weight: 500;
}

.title-bar:before {
  content: '';
  display: inline-block;
  width: 0.375rem;
  height: 1.25rem;
  background: #490097;
  border-radius: 0.375rem;
  margin-right: 0.625rem;
}

.table-wrap table {
  width: 100%;
  border-top: 1px solid #490097;
  border-collapse: collapse;
  border-spacing: 0;
}

.table-wrap table th {
  font-size: 1.125rem;
  font-weight: 500;
  background: #f8faff;
  text-align: center;
  border-bottom: 1px solid #d5d5d5;
  border-left: 1px solid #d5d5d5;
  vertical-align: middle;
  padding: 1rem 0rem;
}

.table-wrap table th:first-child {
  border-left: 0;
}

.table-wrap table th:first-child.line-left {
  border-left: 1px solid #d5d5d5;
}

.table-wrap table td {
  font-size: 1rem;
  color: #787878;
  border-bottom: 1px solid #d5d5d5;
  border-left: 1px solid #d5d5d5;
  vertical-align: middle;
  padding: 1rem 0rem;
}

.table-wrap table td:first-child {
  border-left: 0;
}

.table-wrap table td:first-child.line-left {
  border-left: 1px solid #d5d5d5;
}

.table-wrap.center {
  text-align: center;
}

@media screen and (max-width: 992px) {
  .btn {
    font-size: 1.5rem;
  }
  .btn.btn-gradient {
    width: 20.3125rem;
    height: 4.375rem;
    line-height: 4.375rem;
  }
  .btn.btn-gradient:before {
    margin: -0.375rem;
  }
  .btn.btn-round {
    border-radius: 3.75rem;
  }
  .shadow {
    box-shadow: 0.9375rem 0.9375rem 1.25rem rgba(0, 0, 0, 0.15);
  }
  .page-down {
    position: absolute;
    left: 50%;
    transform: translate(-50%, 0%);
    bottom: 3.125rem;
    width: 5rem;
    height: 5rem;
    background: #a47fcb;
    border-radius: 100%;
    text-align: center;
    z-index: 10;
  }
  .page-down .arrow-d {
    overflow: hidden;
    display: inline-block;
    width: 0.625rem;
    height: 0.625rem;
    border-top: 0.125rem solid #333;
    border-left: 0.125rem solid #333;
    transform: rotate(225deg);
    border-color: #fff;
    width: 1rem;
    height: 1rem;
    border-width: 0.25rem;
    border-radius: 0.25rem;
  }
  .btn-contact {
    left: 1.875rem;
    bottom: 2.5rem;
    width: 6.875rem;
    height: 6.875rem;
    border-radius: 100%;
  }
  .btn-plus {
    width: 3.125rem;
    height: 3.125rem;
    line-height: 3rem;
    font-size: 2.5rem;
  }
  .disc-list {
    display: inline-block;
  }
  .disc-list li {
    position: relative;
    text-align: left;
    padding: 0 0 0 0.9375rem;
    margin: 1.25rem 0;
    font-size: 1.5rem;
    color: #787878;
  }
  .disc-list li:before {
    content: '';
    width: 0.1875rem;
    height: 0.1875rem;
    border-radius: 100%;
    background: #490097;
    position: absolute;
    top: 0.625rem;
    left: 0;
  }
  .title-bar {
    display: block;
    text-align: left;
    font-size: 2rem;
    font-weight: 500;
  }
  .title-bar:before {
    content: '';
    display: inline-block;
    width: 0.5rem;
    height: 1.5rem;
    background: #490097;
    border-radius: 0.5rem;
    margin-right: 1rem;
  }
}

[class*="icon-"] {
  display: inline-block;
  background-repeat: no-repeat;
  background-size: cover;
  text-indent: -9999px;
  overflow: hidden;
}

.icon-menu {
  width: 2.5rem;
  height: 1.875rem;
  background-image: url(../images/icon_menu.png);
}

.icon-close {
  width: 1.875rem;
  height: 1.875rem;
  background-image: url(../images/icon_close.png);
