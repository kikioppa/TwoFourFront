  font-size: 0.875rem;
  color: #fff;
}

.header .pc .top-nav .lang ul li a:hover {
  opacity: 0.8;
}

.header .pc .top-nav .lang.on ul {
  padding: 0.3125rem 0;
  height: auto;
  transition: all 0.3s;
}

.header .mobile {
  display: none;
}

.header:hover:after,
.header.on:after {
  background: white;
  box-shadow: 0 0.625rem 0.625rem rgba(0, 0, 0, 0.2);
}

