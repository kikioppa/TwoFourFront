  padding-top: 47px;
}

.header .pc .top-nav>li {
  display: inline-block;
  color: #fff;
  position: relative;
}

.header .pc .top-nav>li:after {
  content: '';
  position: absolute;
  width: 0.125rem;
  height: 1rem;
  top: 50%;
  right: 0;
  transform: translate(0, -50%);
  background: rgba(255, 255, 255, 0.4);
}

.header .pc .top-nav>li:last-child:after {
  display: none;
}

.header .pc .top-nav>li>a {
  display: block;
  padding: 0 1.1875rem;
  font-weight: 400;
}

.header .pc .top-nav>li>a:hover {
  font-weight: 600;
}

.header .pc .top-nav .lang {
  position: relative;
}

.header .pc .top-nav .lang>span {
