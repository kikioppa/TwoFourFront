  padding: 0 1.5625rem 0 1.1875rem;
  font-weight: 400;
}

.header .pc .top-nav .lang>span:hover {
  font-weight: 600;
}

.header .pc .top-nav .lang>span:after {
  content: '';
  overflow: hidden;
  display: inline-block;
  width: 0.625rem;
  height: 0.625rem;
  border-top: 0.125rem solid #333;
  border-left: 0.125rem solid #333;
  transform: rotate(225deg);
  width: 0.5rem;
  height: 0.5rem;
  position: absolute;
  right: 0.1875rem;
  border-color: #fff;
}

.header .pc .top-nav .lang ul {
  position: absolute;
  width: 100%;
  right: 0;
  top: 1.25rem;
  background-color: cadetblue;
  border-radius: 0.3125rem;
