  background: url(../images/logo.png) no-repeat 0 0;
  text-indent: -9999px;
  overflow: hidden;
}

.header .pc .menu {
  display: inline-block;
  vertical-align: text-top;
  padding: 0px 0 0rem 3.4375rem;
}

.header .pc .menu>li {
  position: relative;
  float: left;
  padding: 47px 2.125rem 23px;
  font-weight: 600;
  font-size: 1.125rem;
}

.header .pc .menu>li:nth-child(1) {
  padding-left: 0;
}

.header .pc .menu>li:nth-child(1) .depth2 {
  margin-left: 0;
}

.header .pc .menu>li:nth-child(1) .depth2 li:nth-child(1) {
  margin-left: -1.012rem;
}

.header .pc .menu>li>a:hover {
  color: #490097;
}

.header .pc .menu>li:hover>.depth2 {
  visibility: visible;
  opacity: 1;
}

.header .pc .menu>li .depth2 {
  opacity: 1;
  visibility: hidden;
  white-space: nowrap;
  position: absolute;
  top: 5.4375rem;
  line-height: 3rem;
  transition: all 0.1s;
  width: 100%;
  margin-left: -2.125rem;
}

.header .pc .menu>li .depth2 li {
  display: inline-block;
  width: 100%;
