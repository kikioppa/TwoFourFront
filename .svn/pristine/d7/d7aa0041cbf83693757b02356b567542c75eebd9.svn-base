.header {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
  width: 100%;
  min-width: 1400px;
  padding: 0 2.5rem;
  font-family: 'Open Sans', sans-serif;
  box-sizing: border-box;
  background-size: 100% 200%;
  color: #222;
  background-color: cadetblue;
}

.header:after {
  content: '';
  display: block;
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(255, 255, 255, 0);
  box-shadow: 0 0.625rem 0.625rem rgba(0, 0, 0, 0);
  z-index: -1;
}

.header .pc {
  display: block;
  max-width: 1600px;
  margin: 0 auto;
}

.header .pc:after {
  display: none;
  content: '';
  position: absolute;
  left: 0;
  top: 5.4rem;
  z-index: -1;
  width: 100%;
  height: 3rem;
  border-bottom: 0.0625rem solid #3d007a;
  background-image: linear-gradient(to right, cadetblue, cadetblue);
  -ms-background-image: linear-gradient(to right, cadetblue, cadetblue);
  -o-background-image: linear-gradient(to right, cadetblue, cadetblue);
  -webkit-background-image: linear-gradient(to right, cadetblue, cadetblue);
  background-color: cadetblue;
}

.header .pc h1 {
  display: inline-block;
  vertical-align: text-top;
  padding: 0.3125rem;
  margin-block-start: 0.5em;
}

.header .pc h1 a {
  display: block;
