@charset "UTF-8";
@import url(https://fonts.googleapis.com/earlyaccess/notosanskr.css);
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video,
input,
textarea {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
  box-sizing: border-box;
}

/* HTML5 display-role reset for older browsers */
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}

body {
  line-height: 1;
}

ol,
ul {
  list-style: none;
}

blockquote,
q {
  quotes: none;
}

blockquote:before, blockquote:after {
  content: "";
  content: none;
}

q:before, q:after {
  content: "";
  content: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

a {
  text-decoration: none;
}

body {
  font-size: 16px;
}

body, textarea {
  font-family: "Noto Sans KR", sans-serif;
}

body {
  height: 100%;
  color: #000000;
}

a {
  color: #000000;
}

pre {
  white-space: pre-wrap;
}

.container {
  min-height: 100%;
  margin-top: 3.125rem;
  padding-bottom: 0.9375rem;
  background-color: #f5f5f5;
}
.container.wedge {
  margin-top: calc(3.125rem + 2.8125rem);
}
.container section {
  background-color: #ffffff;
}

[class*=icon] {
  display: inline-block;
  background-size: contain;
  background-repeat: no-repeat;
}
.icon-menu {
  width: 1.0625rem;
  height: 0.8125rem;
  background-image: url(/images/mobile/ico_menu.png);
}

.icon-notice {
  width: 1.15625rem;
  height: 1.15625rem;
  background-image: url(/images/mobile/ico_notice.png);
}

.icon-faq {
  width: 1.1875rem;
  height: 1.15625rem;
  background-image: url(/images/mobile/ico_faq.png);
}

.icon-arrow {
  width: 0.5rem;
  height: 0.90625rem;
  background-image: url(/images/mobile/ico_arrow.png);
}

.icon-calculator {
  width: 1.28125rem;
  height: 1.1875rem;
  background-image: url(/images/mobile/ico_calculator.png);
}

.icon-kakao {
  width: 1.78125rem;
  height: 1.75rem;
  background-image: url(/images/mobile/ico_kakao.png);
}

.icon-naver {
  width: 1.59375rem;
  height: 1.53125rem;
  background-image: url(/images/mobile/ico_naver.png);
}

.icon-id {
  width: 1.09375rem;
  height: 0.78125rem;
  background-image: url(/images/mobile/ico_id.png);
}

.icon-password {
  width: 0.9375rem;
  height: 0.875rem;
  background-image: url(/images/mobile/ico_password.png);
}

.icon-password-check {
  width: 1rem;
  height: 0.875rem;
  background-image: url(/images/mobile/ico_password_check.png);
}

.icon-name {
  width: 0.8125rem;
  height: 0.875rem;
  background-image: url(/images/mobile/ico_name.png);
}

.icon-phone {
  width: 0.625rem;
  height: 1.0625rem;
  background-image: url(/images/mobile/ico_phone.png);
}

.icon-calendar {
  width: 0.8125rem;
  height: 0.6875rem;
  background-image: url(/images/mobile/ico_calendar.png);
}

.icon-radio {
  width: 0.9375rem;
  height: 0.9375rem;
  background-image: url(/images/mobile/ico_radio.png);
  background-size: cover;
  background-position: bottom;
}

.btn {
  display: block;
  text-align: center;
}
.btn.lg {
  width: 100%;
  padding: 1.09375rem 0;
  font-size: 1rem;
}
.btn.md {
  width: 100%;
  padding: 0.9375rem 0;
  font-size: 0.75rem;
}
.btn.sm {
  padding: 0.3125rem;
}
.btn.primary {
  color: #ffffff;
  background-color: #df0000;
}
.btn.third {
  color: #333333;
  background-color: #ffffff;
  border: 0.0625rem solid #999999;
}
.btn.minor {
  color: #333333;
  background-color: #ffffff;
  border: 0.0625rem solid #999999;
}
.btn.square {
  border-radius: 0.1875rem;
}
.btn.more {
  font-size: 0.75rem;
  color: #999999;
  border: 0.0625rem solid #d5d5d5;
}
.btn.text {
  display: inline-block;
  background-color: unset;
}
.btn.text.primary {
  color: #df0000;
}
.btn.text.sm {
  font-size: 0.75rem;
}
.btn.toggle {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0.5625rem;
  font-size: 0.75rem;
  font-weight: 300;
  border: 0.0625rem solid #d5d5d5;
  border-radius: 0.1875rem;
}

.input-text-wrap {
  width: 100%;
}
.input-text-wrap input[type=text],
.input-text-wrap input[type=password],
.input-text-wrap input[type=number],
.input-text-wrap input[type=tel],
.input-text-wrap input[name=dates],
.input-text-wrap textarea {
  width: 100%;
  height: 100%;
  border: 0;
  box-sizing: border-box;
  font-size: 0.8125rem;
  line-height: 1;
  border-radius: 0;
}
.input-text-wrap input[type=text]::placeholder, .input-text-wrap input[type=text]::-webkit-input-placeholder,
.input-text-wrap input[type=password]::placeholder,
.input-text-wrap input[type=password]::-webkit-input-placeholder,
.input-text-wrap input[type=number]::placeholder,
.input-text-wrap input[type=number]::-webkit-input-placeholder,
.input-text-wrap input[type=tel]::placeholder,
.input-text-wrap input[type=tel]::-webkit-input-placeholder,
.input-text-wrap input[name=dates]::placeholder,
.input-text-wrap input[name=dates]::-webkit-input-placeholder,
.input-text-wrap textarea::placeholder,
.input-text-wrap textarea::-webkit-input-placeholder {
  color: #999999;
  font-size: 0.9375rem;
  font-weight: 300;
  line-height: normal !important;
}
.input-text-wrap textarea {
  padding: 1.40625rem 0.9375rem;
  line-height: 1.6;
  resize: none;
}
.input-text-wrap.underline-type.prefix-icon {
  position: relative;
}
.input-text-wrap.underline-type.prefix-icon input {
  padding: 0.9375rem 0rem 0.9375rem 3.125rem;
}
.input-text-wrap.underline-type.prefix-icon [class*=icon-] {
  position: absolute;
  top: 50%;
  left: 0.9375rem;
  transform: translateY(-50%);
}
.input-text-wrap.underline-type input {
  border-bottom: 0.0625rem solid #d5d5d5;
  padding: 0.9375rem;
}
.input-text-wrap.underline-type input::placeholder {
  font-size: 0.8125rem;
  font-weight: 300;
}

.input-formula {
  display: flex;
}
.input-formula .input-text-wrap {
  flex: 1 auto;
}

.checkbox-label label {
  font-size: 0.8125rem;
  color: #333333;
}

.checkbox-wrap {
  display: inline-block;
  position: relative;
  width: 0.9375rem;
  height: 0.9375rem;
  vertical-align: middle;
  margin-right: 0.78125rem;
}
.checkbox-wrap input[type=checkbox] {
  display: inline-block;
  left: 0;
  top: 0;
  position: absolute;
  height: 0;
  width: 0;
  font-size: 0;
}
.checkbox-wrap input[type=checkbox] + .icon {
  display: inline-block;
  width: 100%;
  height: 100%;
  background-image: url(/images/mobile/ico_checkbox.png);
}
.checkbox-wrap input[type=checkbox]:checked + .icon {
  background-image: url(/images/mobile/ico_checkbox_checked.png);
}
.checkbox-wrap .icon {
  vertical-align: top;
}

a.link-btn-underline {
  display: inline-block;
  padding-bottom: 0.15625rem;
  line-height: 1;
  font-size: 0.625rem;
  color: #787878;
  border-bottom: 0.0625rem solid #bababa;
  vertical-align: bottom;
}

.toggle-wrap {
  border-radius: 0.1875rem;
  border: 0.0625rem solid #d5d5d5;
  overflow: hidden;
}
.toggle-wrap > ul {
  display: flex;
}
.toggle-wrap > ul > li {
  position: relative;
  flex: 1 50%;
  border-right: 0.0625rem solid #d5d5d5;
}
.toggle-wrap > ul > li:last-child {
  border-right: 0;
}
.toggle-wrap > ul > li label {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0.5625rem;
  font-size: 0.75rem;
  font-weight: 300;
}
.toggle-wrap > ul > li input[type=radio] {
  display: inline-block;
  position: absolute;
  width: 0;
  height: 0;
  top: 0;
  left: 0;
}
.toggle-wrap > ul > li input[type=radio]:checked + label {
  background-color: #666666;
  color: #ffffff;
}

.select-wrap {
  position: relative;
}
.select-wrap select {
  border-radius: 0.1875rem;
  border: 0.0625rem solid #d5d5d5;
  padding: 0.9375rem;
  width: 100%;
  font-size: 0.75rem;
  background-color: #ffffff;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}
.select-wrap select::-ms-expand {
  display: none;
}
.select-wrap:after {
  content: "";
  position: absolute;
  width: 0.71875rem;
  height: 0.375rem;
  background-image: url(/images/mobile/ico_select_arrow.png);
  background-repeat: no-repeat;
  background-size: contain;
  right: 0.9375rem;
  top: 50%;
  transform: translateY(-50%);
}

.accordion-list {
  border-bottom: 0.0625rem solid #d5d5d5;
}
.accordion-list > ul > li {
  display: flex;
  border-top: 0.0625rem solid #d5d5d5;
}
.accordion-list > ul > li > a {
  display: flex;
  flex: 1 auto;
  padding: 1.25rem 1.5625rem;
  font-size: 0.9375rem;
  color: #333333;
}
.accordion-list > ul > li > a .title {
  flex: 1 auto;
}
.accordion-list > ul > li > a .icon {
  display: flex;
  flex: 0 0.9375rem;
  justify-content: flex-end;
  align-items: center;
}
.accordion-list > ul > li.has-child.toggle-on .icon svg {
  transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  /* IE 9 */
  -webkit-transform: rotate(180deg);
  /* Safari 3-8 */
}
.accordion-list > ul > li.has-child.toggle-on .icon svg path {
  fill: #999999;
}
.accordion-list > ul > li.child {
  background-color: #f9f9f9;
  padding: 0;
  overflow: hidden;
}
.accordion-list > ul > li.child ul li > a {
  display: flex;
  flex: 1 auto;
  padding: 1.25rem 1.5625rem;
  font-size: 0.8125rem;
  color: #666666;
}
.accordion-list > ul > li.child ul li > a .title {
  flex: 1 auto;
}
.accordion-list > ul > li.child ul li > a .icon {
  display: flex;
  flex: 0 0.9375rem;
  justify-content: flex-end;
  align-items: center;
}

.tab-index {
  border-radius: 0.1875rem;
  border: 0.0625rem solid #d5d5d5;
  overflow: hidden;
}
.tab-index > ul {
  display: flex;
}
.tab-index > ul > li {
  flex: 1 auto;
  border-right: 0.0625rem solid #d5d5d5;
}
.tab-index > ul > li:last-child {
  border-right: 0;
}
.tab-index > ul > li a.item {
  display: flex;
  width: 100%;
  height: 100%;
  padding: 0.84375rem;
  justify-content: center;
  align-items: center;
  background-color: #f9f9f9;
  font-size: 0.875rem;
  color: #333333;
}
.tab-index > ul > li.on a.item {
  background-color: #666666;
  font-size: 0.875rem;
  color: #ffffff;
}

.daterangepicker {
  font-family: "Noto Sans KR", sans-serif;
  top: 158px;
  left: 0px;
  right: 0px;
  width: 100%;
  flex-direction: column;
}
.daterangepicker td.in-range {
  background-color: #ffdfdf;
}
.daterangepicker td.available:hover, .daterangepicker th.available:hover {
  background-color: #ffeeee;
}
.daterangepicker td.active, .daterangepicker td.active:hover {
  background-color: #df0000;
  color: #ffffff;
}
.daterangepicker .drp-calendar {
  flex: 1 1 auto;
  max-width: unset;
}
.daterangepicker .drp-calendar.right, .daterangepicker .drp-calendar.left {
  padding: 0.3125rem;
}
.daterangepicker.opensright:after {
  left: 50%;
}
.daterangepicker.opensright:before {
  left: calc(50% - 1px);
}
.daterangepicker.show-calendar .drp-buttons {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.daterangepicker.show-calendar .drp-buttons .btn {
  display: inline-block;
  border: 0;
  padding: 0.625rem 0.78125rem;
}
.daterangepicker.show-calendar .drp-buttons .btn.cancel {
  color: #333333;
  background-color: #ffffff;
  border: 0.0625rem solid #999999;
}
.daterangepicker.show-calendar .drp-buttons .btn.apply {
  color: #ffffff;
  background-color: #df0000;
}

nav {
  position: fixed;
  top: 0;
  width: 100%;
  background-color: #ffffff;
  height: calc( 3.125rem + 2.8125rem);
  padding-left: 1.5625rem;
  padding-right: 1.5625rem;
  z-index: 2;
}
nav.has-shadow {
  box-shadow: 0rem 0.3125rem 0.3125rem rgba(0, 0, 0, 0.15);
}
nav.has-border {
  border-bottom: 0.0625rem solid #c5c5c5;
}
nav.slide-up {
  top: -3.125rem;
}
nav .header {
  display: flex;
  height: 3.125rem;
}
nav .header > div {
  flex: 1 0 10%;
}
nav .header > div.prefix {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
nav .header > div.prefix a {
  margin-right: 0.625rem;
}
nav .header > div.prefix a:last-child {
  margin-right: 0;
}
nav .header > div.suffix {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
nav .header > div.title {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1 0 70%;
}
nav .header > div.title img {
  width: 4.84375rem;
}
nav .quickmenu {
  height: 2.8125rem;
}
nav .quickmenu > ul {
  display: flex;
  height: 100%;
}
nav .quickmenu > ul > li {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
  flex: 1 auto;
}
nav .quickmenu > ul > li a {
  font-size: 0.875rem;
}
nav.sub {
  height: 3.125rem;
}
nav.sub .header .prefix .icon {
  width: 1.09375rem;
}
nav.sub .header .title {
  font-size: 1rem;
  font-weight: 500;
  color: #333333;
}
nav.sub .quickmenu {
  display: none;
}

.pulldown-menu {
  display: flex;
  flex-direction: column;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: white;
  z-index: 3;
  transition: top 0.5s, bottom 0.5s;
  -webkit-transition: top 0.5s, bottom 0.5s;
}
.pulldown-menu.slide-up {
  top: -100%;
  bottom: 100%;
}
.pulldown-menu .header {
  flex: 0 0 3.125rem;
  display: flex;
  width: 100%;
  padding-left: 1.5625rem;
  padding-right: 1.5625rem;
  background-color: #df0000;
}
.pulldown-menu .header > div {
  flex: 1 auto;
}
.pulldown-menu .header > div.prefix {
  display: flex;
  align-items: center;
}
.pulldown-menu .header > div.prefix > ul > li {
  display: inline-flex;
}
.pulldown-menu .header > div.prefix > ul > li:first-child {
  padding-right: 1.09375rem;
  border-right: 0.0625rem solid #ffa0a0;
}
.pulldown-menu .header > div.prefix > ul > li:last-child {
  padding-left: 1.09375rem;
}
.pulldown-menu .header > div.prefix > ul > li a {
  font-size: 0.75rem;
  color: #ffffff;
  vertical-align: middle;
}
.pulldown-menu .header > div.prefix > ul > li a .icon {
  vertical-align: middle;
}
.pulldown-menu .header > div.suffix {
  flex: 0 0.625rem;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.pulldown-menu .header > div.suffix .btn-close {
  display: inline-block;
  width: 1.5625rem;
  height: 1.5625rem;
  padding: 0.3125rem;
}
.pulldown-menu .header > div.suffix .btn-close span {
  display: inline-block;
}
.pulldown-menu .accordion-list {
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  /* Lets it scroll lazy */
  flex: 1 calc(100% - 6.25rem);
  border-bottom: 0;
}
.pulldown-menu .accordion-list > ul {
  border-bottom: 0.0625rem solid #d5d5d5;
}
.pulldown-menu .accordion-list > ul .child {
  padding-left: 1.5625rem;
}
.pulldown-menu .accordion-list > ul .child > .depth-02 {
  width: 100%;
}
.pulldown-menu .accordion-list > ul .child > .depth-02 > li {
  border-bottom: 0.0625rem solid #d5d5d5;
}
.pulldown-menu .accordion-list > ul .child > .depth-02 > li:last-child {
  border-bottom: 0;
}
.pulldown-menu .bottom-fix {
  padding: 1.5625rem 1.5625rem 4.6875rem;
  width: 100%;
}
.pulldown-menu .bottom-fix ul {
  display: flex;
  height: 100%;
}
.pulldown-menu .bottom-fix ul li {
  display: flex;
  flex: 1 auto;
  justify-content: center;
  align-items: center;
  height: 3.125rem;
}
.pulldown-menu .bottom-fix ul li a {
  display: flex;
  height: 100%;
  width: 100%;
  font-size: 0.75rem;
  font-weight: 500;
  justify-content: center;
  align-items: center;
}
.pulldown-menu .bottom-fix ul li a [class*=icon-] {
  margin-right: 0.625rem;
}
.pulldown-menu .bottom-fix ul li.kakao {
  background-color: #ffeb00;
  margin-right: 0.3125rem;
}
.pulldown-menu .bottom-fix ul li.kakao a {
  color: #333333;
}
.pulldown-menu .bottom-fix ul li.naver {
  background-color: #3eb74d;
}
.pulldown-menu .bottom-fix ul li.naver a {
  color: #ffffff;
}

.main section.main-banner {
  position: relative;
  overflow: hidden;
  background-color: #f5f5f5;
}
.main section.main-banner .owl-carousel button.owl-dot span {
  border: 0.0625rem solid #ffffff;
  background: transparent;
}
.main section.main-banner .owl-carousel button.owl-dot.active span {
  background: #ffffff;
}
.main section.main-banner .owl-carousel.owl-theme .owl-dots {
  margin-top: 0;
  margin-bottom: 0.3125rem;
}
.main section.main-banner .banner .card {
  margin: 1.5625rem auto 0.9375rem;
}
.main section.main-banner .banner .card a {
  display: inline-block;
}
.main section.main-banner .banner .card a img {
  width: 100%;
}
.main section.main-banner .float-btn {
  position: absolute;
  right: -6.25rem;
  bottom: 0;
  z-index: 1;
}
.main section.main-banner .float-btn a {
  height: 9.625rem;
  display: inline-block;
}
.main section.main-banner .float-btn a img {
  height: 100%;
}
.main section.realtime-board {
  padding-left: 1.5625rem;
  padding-right: 1.5625rem;
  padding-bottom: 1.5625rem;
}
.main section.realtime-board .h2 {
  position: relative;
  height: 3.125rem;
}
.main section.realtime-board .h2 img {
  position: absolute;
  left: -0.9375rem;
  top: -0.9375rem;
  width: 15.78125rem;
}
.main section.realtime-board div > ul > li {
  padding: 0.71875rem 0.625rem;
  border-bottom: 0.0625rem solid #d5d5d5;
}
.main section.realtime-board div > ul > li:last-child {
  border-bottom: 0;
}
.main section.realtime-board div > ul > li > ul {
  display: flex;
}
.main section.realtime-board div > ul > li > ul > li {
  flex: 1 0 auto;
  text-align: center;
  color: #999999;
  font-size: 0.6875rem;
  font-weight: 300;
}
.main section.realtime-board div > ul > li > ul > li.name {
  color: #333333;
}
.main section.realtime-board div > ul > li > ul > li.cost {
  color: #333333;
}
.main section.realtime-board div > ul > li > ul > li.result .tag {
  padding: 0.15625rem 0.40625rem;
  border-radius: 0.9375rem;
  font-size: 0.5625rem;
}
.main section.realtime-board div > ul > li > ul > li.result .tag.dark {
  background-color: #333333;
  color: #ffffff;
}
