.section {
  width: 100%;
  font-family: 'Noto Sans KR', sans-serif;
}

.section .section-main {
  width: 1200px;
  margin: auto;
}

.section .section-main h2 {
  font-size: 32px;
  font-weight: 600;
  text-align: center;
  padding: 40px 0px;
  border-bottom: 1px solid rgb(213, 213, 213);
}


/*회원가입 페이지*/

.section .section-main .matters-text {
  border: 1px solid rgb(249, 249, 249);
