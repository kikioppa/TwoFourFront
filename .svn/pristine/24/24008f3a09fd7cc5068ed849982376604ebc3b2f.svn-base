  width: 380px;
}

.section .join_pg .join-check ul li .check {
  display: inline-block;
  vertical-align: sub;
}

.section .join_pg .join-sns {
  border-top: 1px solid rgb(229, 229, 229);
  border-bottom: 1px solid rgb(229, 229, 229);
  padding: 20px 0px;
}

.section .join_pg .join-btn {
  width: 250px;
  margin: auto
}

.section .join_pg .join-btn button {
  background-color: rgb(225, 33, 23);
  border: rgb(225, 33, 23);
  color: white;
  width: 100%;
  height: 60px;
  text-align: center;
  font-size: 18px;
  font-weight: 600;
  margin: 50px 0px 100px 0px;
}


/*약관내용 버튼*/

.section .agree {
  border: 1px solid rgb(189, 189, 189);
  padding: 3px 15px;
  font-size: 14px;
  margin: 0px 7px;
  color: gray;
}

.popup,
.popup2 {
  position: absolute;
  z-index: 500;
  display: none;
  background-color: #ffffff;
  border: 1px solid gray;
  border-radius: 10px;
  font-size: 16px;
}

.pop_title,
.pop_title2 {
  margin: 5px;
  color: black;
  vertical-align: text-bottom;
}

.pop_title button,
.pop_title2 button {
  float: right;
  margin: 0px 5px;
}

.pop_text,
.pop_text2 {
  width: 900px;
  height: 700px;
  overflow-y: scroll;
  background-color: #ffffff;
  border: 1px solid gray;
  margin: 5px;
}

.popback {
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0, 0, 0, 0.5) repeat;
  top: 0;
  left: 0;
  z-index: 10;
  display: none;
}


