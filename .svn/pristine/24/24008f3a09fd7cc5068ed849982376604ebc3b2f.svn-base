}

.section .join_pg .join-check p,
.section .join_pg .join-sns p {
  width: 165px;
  font-weight: 600;
  font-size: 18px;
  display: inline-block;
  color: rgb(51, 51, 51);
}

.section .join_pg .join-check ul,
.section .join_pg .join-sns ul {
  display: inline-block;
  padding-left: 15px;
  font-size: 16px;
  color: rgb(120, 120, 120);
}

.section .join_pg .join-sns ul li:first-child {
  width: 380px;
}

.section .join_pg .join-sns ul li {
  display: inline-block;
}

.section .join_pg .join-check ul li:first-child {
  margin-bottom: 20px;
}

