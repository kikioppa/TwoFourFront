}

.nav {
  width: 100%;
  position: relative;
  margin: auto;
  border: 1px solid rgb(213, 213, 213);
  min-width: 1200px;
  box-sizing: border-box;
}

.gnb {
  display: flex;
  width: 1200px;
  margin: auto;
}

.gnb .li-header {
  padding: 8px 8px;
  width: 400px;
  text-align: center;
  font-size: 1.25rem;
  border-left: 1px solid rgb(213, 213, 213);
  box-sizing: border-box;
  z-index: 10;
}

.gnb .li-header:hover .li-header a {
  color: red;
}

.gnb .li-header:last-child {
  border-right: 1px solid rgb(213, 213, 213);
}

.gnb li a {
  text-align: center;
}

.gnb .on {
  color: red;
}

.gnb>li>a {
  width: 100%;
  height: 80px;
  color: black;
}

.gnb>li:hover>a {
  color: red;
}

.gnb.hover a {
  color: red;
}

.gnb ul {
  display: none;
  position: absolute;
  top: 44px;
  left: 0;
  width: 100%;
  background-color: rgb(245, 245, 245);
  min-width: 1200px;
}

.gnb ul li:hover a {
  border-bottom: 3px solid red;
  padding-bottom: 10px;
}

.gnb ul li {
  margin: 0 25px;
  text-align: center;
  display: inline-block;
}

.gnb ul li a {
  height: 50px;
  padding: 0 7px;
  line-height: 50px;
  font-family: "Noto Sans KR";
}

.gnb>.on ul {
  display: inline;
  z-index: 30;
}

.gnb>.on .on {
  border-bottom: 3px solid red;
  z-index: -99993;
}

.gnb>.on .on :hover {
  border: none;
}

.gnb>.on a {
  position: relative;
}

.gnb>.on a:hover:after {
  width: 100%;
  color: red;
}

.gnb>.on>a {
  color: red;
}


/* .gnb>.on .on:after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
} */

.gnb>.on.hover ul {
  display: block;
  z-index: 30;
}

.gnb>.on.hover ul {
  display: block
}


