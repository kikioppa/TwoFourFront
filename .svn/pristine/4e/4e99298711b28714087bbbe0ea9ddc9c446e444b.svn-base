@charset "utf-8";

.warp{overflow:hidden;width:1000px;margin:40px auto;min-height: 500px;}

.snb{float:left;width:260px;margin-right:30px;}
.snb ul li{border-bottom:1px solid #dcdee2;width:100%;}
.snb ul li a{display:block;padding-left:30px;width:222px;height:52px;line-height:52px;font-size:16px;color:#555;font-weight:bold;}
.snb ul li a span{float:right;padding-right:30px;}

.snb ul li.on{border-bottom:1px solid #333333;background:#333333;}
.snb ul li.on a{color:#fff;}

.contWarp{float:left;width:780px;}
.tabMenu{width:100%;border-top:2px solid #333;}
.tabMenu ul{overflow:hidden;border-bottom:1px solid #dcdee2;;}
.tabMenu ul li{float:left;}
.tabMenu ul li a{display:block;height:50px;line-height:50px;color:#333;font-size:14px;font-weight:bold;padding:0 30px;background:url('../images/common/bar_tab.gif') no-repeat left center;}
.tabMenu ul li:first-child a{background:none;}
.tabMenu ul li.on{background:url('../images/common/ico_tabon.gif') no-repeat center top;}
.tabMenu ul li.on a{color:#1089e6;}

.contents{}
.contents.noTab{margin-top:0px;}

.tableSt_01{border-top:2px solid #333;}
.tableSt_01 th{font-size:14px;color:#555;height:50px;border-bottom:1px solid #dcdee2}
.tableSt_01 td{height:40px;text-align:center;font-size:14px;color:#555;border-bottom:1px solid #dcdee2}
.tableSt_01 td.left{text-align:left;padding-left:10px;}
.tableSt_01 td img{vertical-align:middle;}
.tableSt_01 td span{color:#1089e6;}

.tableSt_01 td .btn{display:inline-block;width:60px;height:28px;line-height:28px;text-align:center;font-size:14px;font-weight:bold;margin:0 10px;}
.tableSt_01 td p{margin-bottom:10px;}
.tableSt_01 td .addFile{border:1px solid #0d4599;color:#0d4599;}
.tableSt_01 td .delFile{border:1px solid #555555;color:#555555;}

.view{border-top:2px solid #333;}
.headSection{height:92px;overflow:hidden;position:relative;border-bottom:1px solid #dcdee2}
.headSection p{padding:15px 0 0 20px;width:640px;}
.headSection p span{padding:0 14px;background:url('../images/common/bar_span.gif') no-repeat left center;font-size:14px;color:#555}
.headSection p span:first-child{padding-left:0px;background:none;}
.headSection p.tit{padding-top:20px;font-size:16px;font-weight:bold;}
.headSection p.tit .ico{display:inline-block;padding:3px 9px;border:1px solid #1089e6;color:#1089e6;border-radius:5px;font-size:13px;line-height:1em;margin-right:10px;}
.headSection p.answer{position:absolute;right:0;top:0;width:115px;height:92px;line-height:92px;border-left:1px solid #dcdee2;font-size:18px;font-weight:bold;color:#1089e6;text-align:center;padding:0;}

.question{padding:20px;font-size:14px;color:#555;border-bottom:1px solid #dcdee2;}

.answerSection{margin:0px 0 0 20px;width:760px;}
.answerArt{margin-top:30px;border-top:2px solid #333;}
.answerArt .tit{padding:16px 20px 16px 43px;overflow:hidden;border-bottom:1px solid #dcdee2;background:url('../images/common/ico_answer.gif') no-repeat 21px center;font-size:16px;font-weight:bold;color:#555;line-height:30px;}
.answerArt .tit .ico_answer{display:inline-block;width:82px;height:30px;line-height:30px;background:#1089e6;color:#fff;border-radius:5px;text-align:center;margin-right:12px;font-size:14px;}
.answerArt .tit b{color:#1089e6;}
.answerArt .tit button{display:inline-block;width:65px;line-height:19px;text-align:center;font-size:13px;color:#555;font-weight:bold;border:1px solid #555;border-radius:5px;margin-top:5px;}
.answerArt .tit .day{float:right;font-size:14px;font-weight:normal;}
.answerCont{padding:20px;border-bottom:1px solid #dcdee2;background:#f9f9f9;font-size:12px;}
.answerCont textarea{border:1px solid #dcdee2;height:172px;width:678px;background:#fff;padding:20px 20px 0 20px;font-size:12px;}
.answerCont textarea.noborder{border:none;height:172px;width:678px;background:transparent;padding:0;font-size:12px;}

.btnSec{margin-top:20px;overflow:hidden;font-size:0px;letter-spacing:0px;word-spacing:0px;}
.btnSec .leftBtn{float:left;}
.btnSec .rightBtn{float:right;}
.btnSec.centerBtn{text-align:center;}
.btnSec .btn{padding:5px 15px;font-size:14px;font-weight:bold;margin-left:12px;}
.btnSec .btn:first-child{margin-left:0px;}
.btnSec .btn.type_01{border:1px solid #0d4599;color:#0d4599;}
.btnSec .btn.type_02{border:1px solid #555555;color:#555555;}
.btnSec .btn.type_02.active{border:1px solid #555555;color:#fff;background:#0d4599;}

.pageSection{position:relative;padding-top: 20px;}
.btnSec.btnLeft{position:absolute;top:0;left:0;margin-top:0px;}

.write{/* margin-top:-30px; */border-top:2px solid #333;}
.tableSt_02{}
.tableSt_02 th{text-align:left;padding-left:20px;font-size:14px;font-weight:bold;border-bottom:1px solid #dcdee2}
.tableSt_02 td{padding:10px 0;border-bottom:1px solid #dcdee2;font-size:14px;}

.tableSt_02 td select{height:30px;width:141px;padding-left:10px;font-size:12px;color:#555;border-radius:5px;border:1px solid #dcdee2;}
.tableSt_02 td input{width:520px;height:28px;font-size:12px;color:#555;border-radius:5px;border:1px solid #dcdee2;margin-left:15px;}
.tableSt_02 td input:first-child{margin-left:0px;}
.tableSt_02 td input.tag{width:101px;}
.tableSt_02 td input.datetime{width:125px;}
.tableSt_02 td input.radio{height: 15px;width: 20px;}
.tableSt_02 td input[type="file"]{width:248px;line-height:28px;}
.tableSt_02 td .btn{display:inline-block;width:60px;height:28px;line-height:28px;text-align:center;font-size:14px;font-weight:bold;margin:0 10px;}
.tableSt_02 td p{margin-bottom:10px;}
.tableSt_02 td .addFile{border:1px solid #0d4599;color:#0d4599;}
.tableSt_02 td .delFile{border:1px solid #555555;color:#555555;}


.member_table{}
.member_table th{text-align:left;padding-left:20px;font-size:14px;font-weight:bold;border-bottom:1px solid #dcdee2}
.member_table th .import{font-size:12px;color:#f16b6b;font-weight:bold;padding-left:2px;}
.member_table td{padding:10px 0;border-bottom:1px solid #dcdee2;font-size:14px;}
.member_table td .resetPasswoed{display:inline-block;line-height:30px;padding-left:12px;color:#888;font-size:14px;}
.member_table td .resetPasswoed b{color:#1089e6}
.member_table td .checkArt label{padding-left:10px;}
.member_table td .checkArt{padding-right:20px;}
.member_table td .checkArt button{font-size:12px;color:#1089e6;line-height:28px;}
.member_table td input{border:1px solid #dcdee2;border-radius:5px;height:28px;font-size:14px;}
.member_table td input.type_01{width:246px;}
.member_table td input.type_02{width:163px;}
.member_table td input.type_03{width:511px;}
.member_table td input.type_04{width:83px;}
.member_table td .inputCenter{padding:0 8px;}
.member_table td p:first-child{padding-top:0;}
.member_table td p{padding-top:10px;}

.member_table td .btn{padding:3px 15px;font-size:14px;font-weight:bold;margin-left:12px;}
.member_table td .btn.type_01{border:1px solid #0d4599;color:#0d4599;margin-left:0px;}
.member_table td .btn.type_02{border:1px solid #555555;color:#555555;}

.passwordNote{padding-top:10px;font-size:14px;color:#888;}
.passwordNote b{color:#1089e6}

.popupSec{width:503px;margin:0 auto;}
.popupSec h1{line-height:54px;background:#333;color:#fff;text-align:center;font-size:16px;font-weight:bold;}
.popupSec .addSec{margin:20px;border-top:2px solid #333}
.popupSec .addSec p{padding:10px 0;border-bottom:1px solid #dcdee2}
.popupSec .addSec p label{display:inline-block;width:80px;line-height:30px;text-align:center;font-seight:bold;}
.popupSec .addSec p input{width:355px;height:28px;border:1px solid #dcdee2;border-radius:5px;margin-left:10px;}
.popupSec .addSec p input.ipt_01{width:324px;margin:0;}
.popupList{width:446px;margin:30px 20px;border-top:2px solid #333}
.popupList th{height:50px;font-weight:bold;color:#555;border-bottom:1px solid #dcdee2}
.popupList td{height:40px;color:#555;text-align:center;border-bottom:1px solid #dcdee2}
.popupList td.leftCont{text-align:left;}
.popupList td button{padding:1px 7px;border:1px solid #555;color:#555;font-weight:bold;}

.popAddress{}
.popAddress .addTab{overflow:hidden;width:100%;line-height:54px;background:#333;}
.popAddress .addTab input{width:16px;height:16px;vertical-align:center;margin:0 15px;}
.popAddress .addTab label{color:#fff;;text-align:center;font-size:16px;font-weight:bold;line-height:20px;}
.addSearch{padding:20px;}
.addSearch input{width:500px;height:28px;border:1px solid #dcdee2;border-radius:5px;}
.addSearch button{padding:3px 15px;font-size:14px;font-weight:bold;margin-left:12px;border:1px solid #0d4599;color:#0d4599;margin-left:20px;border-radius:5px;}
.addSearch p.info{font-size:12px;color:#555}
.warning{font-size:12px;padding-left:20px;}
.addResult{margin-top:20px;}
.addResult table{border-top:2px solid #555}
.addResult table th{height:50px;line-height:50px;font-size:14px;font-weight:bold;border-bottom:1px solid #dcdee2}
.addResult table td{height:40px;font-size:12px;color:#555;text-align:center;}

.addResult table td .textIco{font-size:12px;padding:2px 7px;border-radius:5px;}
.addResult td.leftCont{text-align:left;padding-left:12px;}
.addResult table td .textIco.localName{color:#0972da;border:1px solid #0972da}
.addResult table td .textIco.rodeName{color:#1abdb6;border:1px solid #1abdb6}
.addResult table td button{font-size:12px;padding:2px 7px;border-radius:5px;color:#555;border:1px solid #555}


#ajax_load_indicator{
		z-index: 99999;
	    border: none;
	    margin: 0px;
	    padding: 0px;
	    width: 100%;
	    height: 100%;
	    top: 0px;
	    left: 0px;
	    background-color: rgb(0, 0, 0);
	    opacity: 0.6;
	    cursor: wait;
	    position: fixed;
	    background: url(/images/common/jKEcVPZFk-2.gif ) no-repeat center;
	    display: none;

}

.input-year {width:140px !important;margin-right:10px;}

.popup-wrap {display:none;background:rgba(0,0,0,0.5);position:fixed;width:100%;height:100%;top:0;left:0;z-index:9999;}
.popup-wrap .popup {display:table-cell;vertical-align:middle;}
.popup-wrap .popup-contents {width:100%;max-width:1000px;background-color:#fff;margin:0 auto;padding:30px;}
