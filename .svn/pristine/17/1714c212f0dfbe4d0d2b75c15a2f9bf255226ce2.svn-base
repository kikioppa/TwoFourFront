}

.addFileList li:first-child {
  margin-top: 0;
}

.jquery-filestyle label span {
  display: inline-block;
  width: 100px;
  height: 34px;
  margin: 0 7px;
  line-height: 34px;
  text-align: center;
  color: #fff;
  background-color: #626365
}

.btnAdd {
  margin-right: 6px;
}

.btnAdd,
.btnDel {
  position: relative;
  width: 34px;
  height: 34px;
}

.btnAdd .ir {
  width: 34px;
  height: 34px;
  background: url(../images/bg/bg-btn-add.gif) no-repeat
}

.btnDel .ir {
  width: 34px;
  height: 34px;
  background: url(../images/bg/bg-btn-delete.gif) no-repeat
}

.btnFileDown {
  display: inline-block;
  margin-right: 20px;
}

.btnFileDown img {
  margin: 0 5px 0 0;
  vertical-align: -2px;
}

.checkWrapList li {
  margin-top: 10px
}

.checkWrapList li:first-child {
  margin-top: 0
}

.checkWrap {
  position: relative;
  display: inline-block;
  margin-right: 15px;
}

.checkWrap.mg {
  margin-right: 0;
}

.checkWrap input {
  width: 20px;
  height: 20px;
  opacity: 0
}

.checkWrap label {
  margin-left: -20px;
  font-size: 16px;
  line-height: 16px;
  font-weight: 300
}

.checkWrap label:before {
  width: 20px;
  height: 20px;
  margin-right: 8px;
  vertical-align: -3px;
  overflow: hidden;
  content: '';
  display: inline-block;
  background: url(../images/bg/bg-check.png) no-repeat -30px 0
}

.checkWrap input[type=checkbox]:checked+label:before {
  background-position: -30px -20px
}

.checkWrap input[type=radio]+label:before {
  background-position: -50px 0
}

.checkWrap input[type=radio]:checked+label:before {
  background-position: -50px -20px
}

.checkWrap.big input {
  width: 24px;
  height: 24px;
}

.checkWrap.big label {
  margin-left: -30px;
}

.checkWrap.big label:before {
  width: 24px;
  height: 24px;
  vertical-align: -5px;
  background-position: 0 0;
}

.checkWrap.big input:checked+label:before {
  background-position: 0 -24px
}

.addr>div {
  margin-top: 10px;
}

.addr>div:first-child {
  margin-top: 0;
}


