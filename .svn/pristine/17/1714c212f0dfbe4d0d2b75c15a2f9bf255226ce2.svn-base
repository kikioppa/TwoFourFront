/*admin-mail*/

.mail-tbl {
  width: 1200px;
  margin: auto;
  margin-top: 55px;
  padding-top: 140px;
  position: relative;
}

.contentsWrap article .idea h2 {
  font-weight: 400;
  margin-bottom: 15px;
  text-align: left;
  color: #333;
}

.idea .termSection {
  margin-bottom: 30px;
  display: none;
}

.idea .termSection .term textarea {
  margin-top: 0
}

.ideaTop {
  margin-bottom: 30px;
  border: 1px solid #e0e0e0;
  border-top: 0
}

.ideaTop a {
  display: block;
}

.ideaTop ol {
  width: 770px;
  padding: 30px 69px;
}

.ideaTop li {
  position: relative;
  display: inline-block;
  width: 100px;
  text-align: center;
}

.ideaTop li:before {
  display: block;
  content: '';
  width: 70px;
  height: 70px;
  margin: 0 auto 20px;
  background: url(../images/img/img-idea1.gif) no-repeat center top
}

.ideaTop .step2,
.ideaTop .step3,
.ideaTop .step4 {
  padding-left: 118px;
}

.ideaTop .step2:before {
  background-image: url(../images/img/img-idea2.gif)
}

.ideaTop .step3:before {
  background-image: url(../images/img/img-idea3.gif)
}

.ideaTop .step4:before {
  background-image: url(../images/img/img-idea4.gif)
}

.ideaTop li:after {
  position: absolute;
  top: 35px;
  right: -80px;
  display: block;
  content: '';
  width: 38px;
  height: 38px;
  background: url(../images/bg/bg-idea.gif) no-repeat center top
}

.ideaTop .step4:after {
  display: none
}

.ideaTop p {
  width: 888px;
  padding: 10px;
  text-align: center;
  color: #666;
  font-weight: 300;
  font-size: 14px;
  background-color: #fafafa
}

.ideaTop p em {
  font-weight: 400
}

.ideaBottom {
  margin-top: 30px;
  text-align: center;
}

.ideaBottom a {
  display: inline-block;
  width: 224px;
  height: 50px;
  line-height: 50px;
  margin-bottom: 20px;
  font-size: 18px;
  color: #fff;
  background-color: #0061a9;
}

.ideaBottom a:after {
  display: inline-block;
  content: '';
  width: 8px;
  height: 12px;
  margin-left: 10px;
  background: url(../images/bg/bg-bul3.png) no-repeat center top
}

.ideaBottom p {
  font-size: 14px;
  font-weight: 300;
}

.ideaBottom p.noti {
  margin-top: 10px;
  color: #999;
}


