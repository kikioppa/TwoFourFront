/* amdin-client-info-p2*/

.modal-wrap.is-visible {
  visibility: visible;
  opacity: 1;
}

.modal-wrap {
  visibility: hidden;
  opacity: 0;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2000;
  background: rgba(0, 0, 0, 0.5);
  width: 100vw;
  height: 100vh;
  transition: all 0.3s;
}

.modal-wrap .modal {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  min-width: 31.25rem;
  min-height: 20.625rem;
  max-width: 90vw;
  max-height: 90vh;
  padding: 3.125rem;
  box-sizing: border-box;
  background: #fff;
  border-radius: 0.25rem;
  overflow-y: auto;
}

.modal-wrap .modal .title {
  font-size: 1.25rem;
  font-weight: 500;
  color: #490097;
  border-bottom: 0.0625rem solid #d5d5d5;
  margin: -1.25rem -3.125rem 0;
  padding: 0rem 1.875rem 1.5625rem;
  text-align: center;
}

.icon-x {
  display: inline-block;
  background-repeat: no-repeat;
  background-size: cover;
  overflow: hidden;
  text-indent: -9999px;
}

.modal-wrap .modal .con .btn {
  width: 100%;
}

.btn.btn-primary {
  color: black;
  border: 1px solid black;
}

.btn.btn-round {
  border-radius: 1.875rem;
  overflow: hidden;
}

.btn.btn-default {
  width: 18.125rem;
  padding: 1.25rem 0;
  font-size: 1.25rem;
  font-weight: 500;
}

.btn {
  display: inline-block;
  text-align: center;
}

.reset-btn {
  background-color: white;
  border: 1px solid #d5d5d5;
  border-radius: 5px;
  padding: 5px 5px;
  margin-top: 15px;
}

.get-info {
  text-align: center;
  background-color: white;
  border: 1px solid gray;
  margin-top: 15px;
}

.info-text {
  display: flex;
  margin-top: 15px;
  margin-bottom: 15px;
}

.info-text .user-info {
  padding-right: 15px;
}

.info-text .user-info input {
  background-color: unset;
  border: 1px solid #d5d5d5;
  border-radius: 2px;
  width: 100px;
  text-align: center;
}

.info-text .phone-info input {
  background-color: unset;
  border: 1px solid #d5d5d5;
  border-radius: 2px;
  width: 280px;
  text-align: center;
}

[class*="icon-"] {
  display: inline-block;
  background-repeat: no-repeat;
  background-size: cover;
  overflow: hidden;
}

.modal textarea {
  border: 1px solid #d5d5d5;
