.section.notice {
  height: 620px;
  background-color: #ffffff;
  margin: auto;
  width: 1200px;
  position: relative;
}

.notice-header {
  width: 700px;
  height: 59px;
  font-weight: 300;
  /* border-top: 1px solid rgb(213, 213, 213); */
  border-bottom: 1px solid rgb(213, 213, 213);
  font-size: 1.25rem;
}

.notice-header span,
.notice-header a {
  position: relative;
  float: left;
  width: 20%;
  height: 60px;
  line-height: 20px;
  padding: 20px 0;
  text-align: center;
  color: #878787;
  font-weight: 300;
  border-right: 1px solid rgb(213, 213, 213);
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  cursor: pointer;
}

.notice-header span {
  width: 30%;
  font-weight: 400;
}

.notice-header a {
  border-right: none;
  font-size: 0.9375rem;
}

.section .notice-header .on {
  color: red;
  cursor: pointer;
  background-color: white;
  border-right: 1px solid rgb(213, 213, 213);
}

.notice-header a:last-child {
  background-color: unset;
  float: right;
}

.notice-list {
  width: 700px;
  border: 1px solid rgb(213, 213, 213);
  position: absolute;
  top: -60px;
  left: 0;
}
.notice-list a{
  padding-left: 10px;
}

.notice-list .faq-list {
  display: none;
}


