html,
body {
  margin: 0;
  padding: 0;
  height: 100%;
  font-family: 'Noto Sans Korean', sans-serif;
}


dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
tbody,
tfoot,
thead,
th,
td {
  margin: 0;
  padding: 0
}

td,
th {
  color: #333;
  vertical-align: middle
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: normal;
  font-size: 100%
}

table {
  width: 100%;
  table-layout: fixed;
  border-spacing: 0;
}

ol,
ul,
li {
  list-style: none
}

hr {
  position: absolute;
  left: -5000px;
  top: 0;
  visibility: hidden;
  overflow: hidden;
  line-height: 0.1%;
  font-size: 0.1%;
  width: 0.1%;
  height: 0.1%
}

img {
  max-width: 100%;
}

img,
fieldset {
  border: 0
}

img {
  vertical-align: top
}

table img {
  vertical-align: middle
}

button {
  overflow: visible;
  margin: 0;
  padding: 0;
  border: 0;
  background: none;
  font-size: 0;
  line-height: 0;
  font-size: 0px;
  letter-spacing: 0px;
  word-spacing: 0px;
  cursor: pointer;
  background: none;
  vertical-align: top;
  text-align: left;
  font: 14px/22px 'NanumSquare', sans-serif;
  outline: none;
}

input,
textarea,
select,
label {
  font: 13px/25px 'NanumSquare', sans-serif;
  color: #333;
  vertical-align: middle;
}
select::-ms-expand {
  display: none;
}
input::-ms-clear{
  display: none;
}
address {
  font-style: normal
}

a {
  text-decoration: none;
  color: #333;
}

a:hover,
a:focus {
  text-decoration: none;
}

legend,
caption {
  visibility: hidden;
  overflow: hidden;
  width: 0;
  height: 0;
  font-size: 0;
  line-height: 0;
  padding: 0;
  margin: 0;
}


/*header*/

.header {
  width: 100%;
  min-width: 1200px;
  margin: auto;
  margin-top: 2rem;
}

.wrapper {
  width: 1200px;
  position: relative;
  margin: auto;
}

.header .left img {
  float: left;
}

.header .logo {
  display: inline-block;
  margin: 30px;
  margin-left: 268px
}

.header .logo img {
  vertical-align: bottom
}

.header .right {
  float: right;
  vertical-align: middle;
}

.header .right ul {
  display: flex;
  margin-top: 10px
}

.header .right ul li {
  padding-left: 8px;
}

.join {
  background-image: url(/images/front/join.png);
  height: 30px;
  display: flex;
  background-repeat: no-repeat;
  background-position: center;
}

.login {
  background-image: url(/images/front/login.png);
  height: 30px;
  display: flex;
  background-repeat: no-repeat;
  background-position: center;
}

.logout {
  background-image: url(/images/front/ico_logout.png);
  height: 30px;
  display: flex;
  background-repeat: no-repeat;
  background-position: center;
}

.mypage {
  background-image: url(/images/front/mypage.png);
  height: 30px;
  display: flex;
  background-repeat: no-repeat;
  background-position: center;
}

.nav {
  width: 100%;
  position: relative;
  margin: auto;
  border-top: 1px solid rgb(213, 213, 213);
  border-bottom: 1px solid rgb(213, 213, 213);
  min-width: 1200px;
  box-sizing: border-box;
}

.gnb {
  display: flex;
  width: 1200px;
  margin: auto;
}

.gnb .li-header {
  padding: 8px 8px;
  width: 400px;
  text-align: center;
  font-size: 1.25rem;
  border-left: 1px solid rgb(213, 213, 213);
  box-sizing: border-box;
  z-index: 10;
}

.gnb .li-header:hover .li-header a {
  color: red;
}

.gnb .li-header:last-child {
  border-right: 1px solid rgb(213, 213, 213);
}

.gnb li a {
  text-align: center;
}

.gnb .on {
  color: red;
}

.gnb>li>a {
  width: 100%;
  height: 80px;
  color: black;
}

.gnb>li:hover>a {
  color: red;
}

.gnb.hover a {
  color: red;
}

.gnb ul {
  /* display: none; */
  /* position: absolute;
  top: 44px;
  left: 0; */
  width: 100%;
  background-color: rgb(245, 245, 245);
  min-width: 1200px;
}
.gnb-2depth{
  border-top: 1px solid #d5d5d5;
  background-color: rgb(245, 245, 245);
}
.gnb-2depth ul {
  width: 1200px;
  margin: 0 auto;
  text-align: center;
}
.gnb-2depth ul li{
  display: inline-block;
}
.gnb-2depth ul li a{
  display: inline-block;
  font-size: 18px;
  padding: 20px 36px;
  box-sizing: border-box;
}
.gnb-2depth ul li.on a{
  border-bottom: 3px solid red;

}
.gnb ul li:hover a {
  border-bottom: 3px solid red;
  padding-bottom: 10px;
}

.gnb ul li {
  margin: 0 25px;
  text-align: center;
  display: inline-block;
}

.gnb ul li a {
  height: 50px;
  padding: 0 7px;
  line-height: 50px;
  font-family: 'Noto Sans Korean';
}

.gnb>.on ul {
  display: inline;
  z-index: 30;
}

.gnb>.on .on {
  border-bottom: 3px solid red;
  z-index: -99993;
}

.gnb>.on .on :hover {
  border: none;
}

.gnb>.on a {
  position: relative;
}

.gnb>.on a:hover:after {
  width: 100%;
  color: red;
}

.gnb>.on>a {
  color: red;
}


/* .gnb>.on .on:after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
} */

.gnb>.on.hover ul {
  display: block;
  z-index: 30;
}

.gnb>.on.hover ul {
  display: block
}


