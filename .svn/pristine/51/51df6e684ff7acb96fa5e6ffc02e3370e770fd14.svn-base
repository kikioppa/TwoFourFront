}

.main-banner.owl-carousel .owl-nav button.owl-prev {
  position: absolute;
  left: 170px;
  top: 210px;
  background-image: url(/images/front/prev.png);
  width: 43px;
  height: 83px;
  text-indent: -9999px;
}

.main-banner .main-dot {
  position: absolute;
  left: 50%;
  bottom: 50px;
}

.main-banner .main-dot span {
  color: rgb(141, 159, 217);
}

.main-banner .main-dot button.owl-dot {
  border: 1px solid white;
